{
  "version": 2,
  "builds": [
    {
      "src": "slizlik-app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "slizlik-app/build"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "/slizlik-app/build/static/$1"
    },
    {
      "source": "/assets/(.*)",
      "destination": "/slizlik-app/public/assets/$1"
    },
    {
      "source": "/favicon.ico",
      "destination": "/slizlik-app/public/favicon.ico"
    },
    {
      "source": "/(.*)",
      "destination": "/slizlik-app/build/index.html"
    }
  ]
}