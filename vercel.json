{
  "version": 2,
  "buildCommand": "cd slizlik-app && git lfs install && git lfs pull && npm install && npm run build",
  "outputDirectory": "slizlik-app/build",
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "/static/$1"
    },
    {
      "source": "/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/favicon.ico",
      "destination": "/favicon.ico"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}