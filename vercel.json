{
  "version": 2,
  "builds": [
    {
      "src": "slizlik-app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build",
        "zeroConfig": true
      }
    }
  ],
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "/static/$1"
    },
    {
      "source": "/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/favicon.ico",
      "destination": "/favicon.ico"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}